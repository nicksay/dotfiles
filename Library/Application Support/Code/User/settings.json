{
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
  "clang-format.executable": "/usr/local/bin/clang-format",
  "clang-format.fallbackStyle": "Google",
  "clang-format.style": "Google",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Source Code Pro, Monaco, monospace",
  "editor.fontSize": 13,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "files.associations": {
    "PULLREQ_EDITMSG": "markdown",
    "*.asciiproto": "prototxt",
    "*.gss": "css",
    "*.pbtxt": "prototxt",
    "*.sdl": "sql",
    "*.pb": "prototxt",
    "*.textproto": "prototxt"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": false,
  "go.useLanguageServer": true,
  "html.format.enable": false,
  "java.format.enabled": false,
  "javascript.format.enable": false,
  "python.defaultInterpreterPath": "/Users/nicksay/.pyenv/shims/python",
  "python.formatting.provider": "yapf",
  "python.languageServer": "Pylance",
  "python.linting.pylintEnabled": true,
  "python.pythonPath": "/Users/nicksay/.pyenv/shims/python",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Source Code Pro, Monaco, monospace",
  "terminal.integrated.fontSize": 13,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#252525",
    "statusBar.noFolderBackground": "#252525",
    "statusBar.debuggingBackground": "#511f1f",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#AC4142",
    "terminal.ansiGreen": "#90A959",
    "terminal.ansiYellow": "#F4BF75",
    "terminal.ansiBlue": "#6A9FB5",
    "terminal.ansiMagenta": "#AA759F",
    "terminal.ansiCyan": "#75BFAA",
    "terminal.ansiWhite": "#D0D0D0",
    "terminal.ansiBrightBlack": "#505050",
    "terminal.ansiBrightRed": "#AC4142",
    "terminal.ansiBrightGreen": "#90A959",
    "terminal.ansiBrightYellow": "#F4BF75",
    "terminal.ansiBrightBlue": "#6A9FB5",
    "terminal.ansiBrightMagenta": "#AA759F",
    "terminal.ansiBrightCyan": "#75BFAA",
    "terminal.ansiBrightWhite": "#F5F5F5"
  },
  "workbench.colorTheme": "Base16 Dark Ocean",
  "workbench.editor.tabSizing": "shrink",
  "workbench.startupEditor": "newUntitledFile",
  "[bazel]": {
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[git-commit]": {
    "editor.rulers": [
      72,
      80
    ]
  },
  "[html]": {
    "editor.rulers": [
      100
    ]
  },
  "[java]": {
    "editor.rulers": [
      100
    ],
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.tabSize": 4
  },
  "[objective-c]": {
    "editor.rulers": [
      100
    ],
  },
  "[python]": {
    "editor.tabSize": 4
  }
}
