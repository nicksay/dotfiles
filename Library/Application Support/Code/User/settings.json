{
  "editor.fontFamily": "Source Code Pro, Monaco, monospace",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.rulers": [
    80
  ],
  "files.insertFinalNewline": true,
  "javascript.format.enable": false,
  "python.formatting.provider": "yapf",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Source Code Pro, Monaco, monospace",
  "terminal.integrated.fontSize": 13,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#252525",
    "statusBar.noFolderBackground": "#252525",
    "statusBar.debuggingBackground": "#511f1f",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#AC4142",
    "terminal.ansiGreen": "#90A959",
    "terminal.ansiYellow": "#F4BF75",
    "terminal.ansiBlue": "#6A9FB5",
    "terminal.ansiMagenta": "#AA759F",
    "terminal.ansiCyan": "#75BFAA",
    "terminal.ansiWhite": "#D0D0D0",
    "terminal.ansiBrightBlack": "#505050",
    "terminal.ansiBrightRed": "#AC4142",
    "terminal.ansiBrightGreen": "#90A959",
    "terminal.ansiBrightYellow": "#F4BF75",
    "terminal.ansiBrightBlue": "#6A9FB5",
    "terminal.ansiBrightMagenta": "#AA759F",
    "terminal.ansiBrightCyan": "#75BFAA",
    "terminal.ansiBrightWhite": "#F5F5F5"
  },
  "workbench.colorTheme": "Base16 Dark Ocean",
  "workbench.startupEditor": "newUntitledFile",
  "[git-commit]": {
    "editor.rulers": [
      72,
      80
    ]
  },
  "[markdown]": {
    "editor.rulers": [
      72,
      80
    ]
  },
  "[python]": {
    "editor.tabSize": 4
  }
}
